{
  "dependencies": {
    "discord.js": "^14.14.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "prepare": "husky",
    "serve": "bun run src/index.ts",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "bun run lint -- --fix",
    "fmt": "prettier --write ./src/**/*.ts",
    "refreshCmds": "bun run src/index.ts --rc"
